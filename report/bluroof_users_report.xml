<?xml version="1.0" ?>
<odoo>
   <report
        id="bluroof_guests_report"
        model="bluroof.guest"
        string="Exam Session report"
        report_type="qweb-pdf"
        name="bluroof.bluroof_users_report"
        file="bluroof.bluroof_users_report"
    />
    <template id="bluroof_users_report">
        <t t-call="report.html_container">
            <t t-foreach="docs" t-as="e">
                <t t-call="report.external_layout">
                    <div class="page">
                        <div class="oe_structure"/>
                        <br>
                            <br>
                                <table class="table table-condensed">
                                    <tr> 
                                        <th>Name</th>
                                        <th>BirthDate</th>
                                        <th>Actual State</th>          
                                    </tr>
                              
                                    <tr>
                                        <td>
                                            <br>
                                                <span t-field="e.name"/>
                                            </br>
                                        </td>
                                        <td>
                                            <br>
                                                <span t-field="e.birthDate"/>
                                            </br>
                                        </td>
                                        <td>
                                            <br>
                                                <span t-field="e.actualState"/>
                                            </br>
                                        </td>
                               
                                    </tr>
                                </table>  
                            </br>
                           </br>
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>
